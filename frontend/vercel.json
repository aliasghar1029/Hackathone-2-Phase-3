{
  "version": 2,
  "framework": "nextjs",
  "functions": {
    "app/**/*": {
      "maxDuration": 60
    }
  },
  "env": {
    "NEXT_PUBLIC_API_BASE_URL": "https://white-000-cahtbot.hf.space",
    "NEXT_PUBLIC_API_VERSION": "v1"
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        }
      ]
    }
  ]
}